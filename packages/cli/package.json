{
  "name": "quick-start-cli",
  "version": "1.0.0",
  "description": "Cli for quickly generating project templates",
  "type": "module",
  "bin": {
    "quick-start": "./dist/index.js"
  },
  "exports": "./dist/index.js",
  "scripts": {
    "dev": "yarn tsup && node dist/index.js"
  },
  "keywords": [
    "cli"
  ],
  "author": "xuanxin",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "tsup": "^8.0.2",
    "zod": "^3.22.4"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "format": "esm",
    "target": "esnext"
  }
}
